<body>
<div class="container">
  {% for order in orders %}
  <div style="page-break-after: always;">
    <div style="margin-bottom: 20px;">
      <img src="data:image/png;base64," class="icon-logo">
    </div>
    <h1>{{ text_invoice }} #{{ order.order_id }}</h1>
    <table class="table table-bordered">
      <thead>
        <tr>
          <td colspan="2">{{ text_order_detail }}</td>
        </tr>
      </thead>
      <tbody>
        <tr>
          <td style="width: 50%;">
            <address>
              <strong>NUTRAS.DE</strong><br />
            </address>
            <b>{{ text_email }}</b>&nbsp;nutrasstore@gmail.com<br />
            <b>{{ text_website }}</b> https://nutras.de/<br />
            <b>Registered office address:</b><br /> 85 Great Portland Street, First Floor, London, England, W1W 7LT</td>
          <td style="width: 50%;"><b>{{ text_date_added }}</b>&nbsp;{{ order.date_added }}<br />
            <b>{{ text_order_id }}</b>&nbsp;{{ order.order_id }}<br />
            <b>{{ text_payment_method }}</b>&nbsp;{{ order.payment_method }}<br />
            {% if order.shipping_method %}
            <b>{{ text_shipping_method }}</b>&nbsp;{{ order.shipping_method }}<br />
            {% endif %}</td>
        </tr>
      </tbody>
    </table>
    <table class="table table-bordered">
      <thead>
        <tr>
          <td style="width: 50%;"><b>Payment details</b></td>
          <td style="width: 50%;"><b>{{ text_shipping_address }}</b></td>
        </tr>
      </thead>
      <tbody>
        <tr>
          <td><b>IBAN:</b> BE17 9672 8470 0521<br>
              <b>BIC:</b> TRWIBEB1XXX<br>
              <b>Name der Firma:</b> Nutraviso Limited<br>
              <b>Firmenanschrift:</b> 85 Great Portland Street, London, W1W 7LT
          </td>
          <td><address>
            {{ order.shipping_address }}
            </address></td>
        </tr>
      </tbody>
    </table>
    <table class="table table-bordered">
      <thead>
        <tr>
          <td><b>{{ column_product }}</b></td>
          <td><b>{{ column_model }}</b></td>
          <td class="text-right"><b>{{ column_quantity }}</b></td>
          <td class="text-right"><b>{{ column_price }}</b></td>
          <td class="text-right"><b>{{ column_total }}</b></td>
        </tr>
      </thead>
      <tbody>
        {% for product in order.product %}
        <tr>
          <td>{{ product.name }}</td>
          <td>{{ product.model }}</td>
          <td class="text-right">{{ product.quantity }}</td>
          <td class="text-right">{{ product.price }}</td>
          <td class="text-right">{{ product.total }}</td>
        </tr>
        {% endfor %}
        {% for voucher in order.voucher %}
        <tr>
          <td>{{ voucher.description }}</td>
          <td></td>
          <td class="text-right">1</td>
          <td class="text-right">{{ voucher.amount }}</td>
          <td class="text-right">{{ voucher.amount }}</td>
        </tr>
        {% endfor %}
        {% for total in order.total %}
        <tr>
          <td class="text-right" colspan="4"><b>{{ total.title }}</b></td>
          <td class="text-right">{{ total.text }}</td>
        </tr>
        {% endfor %}
      </tbody>
    </table>
  </div>
  {% endfor %}
</div></body></html>